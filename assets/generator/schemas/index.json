{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "Hello world schema",
    "description": "Hello world schema to describe code generation data model.",
    "default": {},
    "examples": [
        {
            "who": "World"
        }
    ],
    "required": [
        "who"
    ],
    "properties": {
        "who": {
            "$id": "#/properties/who",
            "type": "string",
            "title": "Hello 'who'?",
            "description": "Who do you want to say hello to?",
            "default": "World",
            "examples": [
                "World"
            ]
        }
    },
    "additionalProperties": false
}