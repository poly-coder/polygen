  },
  "dependencies": {
    "chalk": "^4.1.0",
    "change-case": "^4.1.1",
    "commander": "^6.1.0",
    "consola": "^2.15.0",
    "ejs": "^3.1.5",
    "execa": "^4.0.3",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.6",
    "humanize-plus": "^1.8.2",
    "inflection": "^1.12.0",
    "ini": "^1.3.5",
    "inquirer": "^7.3.3",
    "js-yaml": "^3.14.0",
    "json5": "^2.1.3",
    "jsonpath-plus": "^4.0.0",
    "liquidjs": "^9.15.0",
    "mustache": "^4.0.1",
    "nunjucks": "^3.2.2",
    "pug": "^3.0.0",
    "time-require": "^0.1.2",
    "toml": "^3.0.0",
    "xml2js": "^0.4.23",
    "yargs-parser": "^20.0.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.4",
    "@types/fs-extra": "^9.0.1",
    "@types/humanize-plus": "^1.8.0",
    "@types/inflection": "^1.5.28",
    "@types/ini": "^1.3.30",
    "@types/jest": "^26.0.14",
    "@types/js-yaml": "^3.12.5",
    "@types/json5": "^0.0.30",
    "@types/mustache": "^4.0.1",
    "@types/node": "^14.10.3",
    "@types/nunjucks": "^3.1.3",
    "@types/pug": "^2.0.4",
    "@types/xml2js": "^0.4.5",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2",
    "pkg": "^4.4.9",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/__tests__/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": "src/**/*",
    "outPath": "standalone",
    "targets": [
      "node12-macos",
      "node12-linux",
      "node12-win"
    ]
  }
}
